{
  "name": "alfred-chatbot",
  "version": "0.0.1",
  "description": "WhatsApp Chatbot",
  "type": "module",
  "scripts": {
    "dev": "node --es-module-specifier-resolution=node src/main.js",
    "dev:detach": "node --es-module-specifier-resolution=node src/main.js >> logs/console.log 2>&1 &",
    "log:dev": "tail -f logs/console.log",
    "http-ctrl:dev": "node --es-module-specifier-resolution=node src/httpCtrl.js",
    "http-ctrl:dev:detach": "node --es-module-specifier-resolution=node src/httpCtrl.js >> logs/http-ctrl-console.log 2>&1 &",
    "log:http-ctrl:dev": "tail -f logs/http-ctrl-console.log",
    "start": "pm2 start src/main.js --node-args='--es-module-specifier-resolution=node' --name alfred-bot-new ",
    "stop": "pm2 stop src/main.js",
    "restart": "pm2 restart src/main.js",
    "reload": "pm2 reload alfred-bot-new",
    "log": "pm2 logs alfred-bot-new",
    "http-ctrl:start": "pm2 start src/httpCtrl.js --node-args='--es-module-specifier-resolution=node'--name alfred-bot-front ",
    "http-ctrl:stop": "pm2 stop alfred-bot-front",
    "http-ctrl:restart": "pm2 restart alfred-bot-front",
    "http-ctrl:reload": "pm2 reload alfred-bot-front",
    "http-ctrl:log": "pm2 logs alfred-bot-front",
    "conversations": "tail -f logs/conversations.log",
    "test": "jest"
  },
  "keywords": [
    "nodejs",
    "whatsapp",
    "chatbot",
    "venom",
    "docker"
  ],
  "dependencies": {
    "child_process": "^1.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.3",
    "fs": "^0.0.1-security",
    "http": "^0.0.1-security",
    "mysql2": "^2.3.3",
    "node-fetch": "^3.1.0",
    "nodemailer": "^6.8.0",
    "path": "^0.12.7",
    "sequelize": "^6.28.0",
    "sync-fetch": "^0.4.1",
    "venom-bot": "^4.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "babel-jest": "^28.1.3",
    "jest": "^28.1.3"
  },
  "main": "main.js"
}
